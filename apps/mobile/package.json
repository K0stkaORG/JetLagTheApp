{
    "name": "mobile",
    "main": "index.js",
    "version": "1.0.0",
    "scripts": {
        "dev": "expo start -c --dev-client",
        "prebuild": "pnpm exec expo prebuild",
        "build:clear": "pwsh ./scripts/clear.ps1",
        "build:dev": "pwsh ./scripts/build-dev.ps1",
        "install:dev": "adb install -r ./output/app-debug.apk",
        "build:prod": "pwsh ./scripts/build-prod.ps1",
        "install:prod": "adb install -r ./output/app-release.apk",
        "postinstall": "pnpm exec tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "emulator": "pwsh ./scripts/emulator.ps1"
    },
    "dependencies": {
        "@maplibre/maplibre-react-native": "^10.1.6",
        "@react-native-async-storage/async-storage": "^2.2.0",
        "@react-navigation/native": "^7.1.11",
        "@rn-primitives/alert-dialog": "^1.2.0",
        "@rn-primitives/avatar": "~1.2.0",
        "@rn-primitives/dialog": "^1.2.0",
        "@rn-primitives/portal": "~1.3.0",
        "@rn-primitives/progress": "~1.2.0",
        "@rn-primitives/slot": "~1.2.0",
        "@rn-primitives/tabs": "^1.2.0",
        "@rn-primitives/tooltip": "~1.2.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "expo": "~53.0.12",
        "expo-asset": "~11.1.5",
        "expo-file-system": "~18.1.10",
        "expo-linking": "~7.1.5",
        "expo-navigation-bar": "~4.2.5",
        "expo-router": "~5.0.7",
        "expo-secure-store": "~14.2.3",
        "expo-splash-screen": "~0.30.9",
        "expo-status-bar": "~2.2.3",
        "expo-system-ui": "~5.0.8",
        "lucide-react-native": "^0.511.0",
        "nativewind": "^4.1.23",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-native": "0.79.4",
        "react-native-gesture-handler": "~2.24.0",
        "react-native-reanimated": "~3.18.0",
        "react-native-safe-area-context": "^5.4.1",
        "react-native-screens": "~4.11.1",
        "react-native-svg": "^15.12.0",
        "react-native-web": "~0.20.0",
        "react-native-worklets": "^0.7.1",
        "socket.io-client": "^4.8.1",
        "sonner-native": "^0.20.0",
        "tailwind-merge": "^2.6.0",
        "tailwindcss": "3.3.5",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.25.64"
    },
    "devDependencies": {
        "@babel/core": "^7.27.4",
        "@types/react": "~19.0.14",
        "prettier": "^3.5.3",
        "prettier-plugin-tailwindcss": "^0.6.12",
        "typescript": "^5.8.3"
    },
    "private": true
}
