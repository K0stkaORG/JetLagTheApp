{
    "expo": {
        "name": "JetLag: The App",
        "slug": "jet-lag-the-app",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "jet-lag-the-app",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#213042"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "com.krychlic.jetlagtheapp"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/icon.png"
        },
        "plugins": ["expo-router", "expo-secure-store", "expo-asset"],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {}
        },
        "owner": "krychlic"
    }
}
